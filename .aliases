alias update-git-timestamp='git commit --amend --date="$(date -R)"'
alias bot="git config credential.helper store; export GIT_AUTHOR_NAME=bot; export GIT_AUTHOR_EMAIL=bot@users.noreply.github.com; export GIT_COMMITTER_NAME=bot; export GIT_COMMITTER_EMAIL=bot@users.noreply.github.com"
alias myself="git config --unset credential.helper; unset GIT_AUTHOR_NAME; unset GIT_AUTHOR_EMAIL; unset GIT_COMMITTER_NAME; unset GIT_COMMITTER_EMAIL"
